<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">

    <PreferenceCategory android:title="@string/menu_rclone">
        <Preference
            android:icon="@drawable/prefs_rclone_white_22dp"
            android:key="@string/settings_rclone_import_config"
            android:persistent="false"
            android:summary="@string/menu_rclone_import_config_summary"
            android:title="@string/menu_rclone_import_config" />

        <Preference
            android:icon="@drawable/ic_rclone_cloud"
            android:key="@string/settings_rclone_config"
            android:persistent="false"
            android:summary="@string/menu_rclone_import_config_not_imported" />
    </PreferenceCategory>

    <PreferenceCategory android:title="@string/menu_rclone_backup_title">
        <Preference
            android:icon="@drawable/ic_cloud_upload_white_24dp"
            android:key="@string/settings_rclone_backup"
            android:persistent="false"
            android:summary="@string/menu_rclone_backup_not"
            android:title="@string/menu_rclone_backup" />
    </PreferenceCategory>

    <PreferenceCategory android:title="@string/menu_rclone_download">
        <Preference
            android:icon="@drawable/ic_cloud_download_white_24dp"
            android:key="@string/settings_rclone_download"
            android:persistent="false"
            android:summary="@string/menu_rclone_download_backup_summary"
            android:title="@string/menu_rclone_download_backup" />
    </PreferenceCategory>
</PreferenceScreen>
